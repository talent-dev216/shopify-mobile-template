{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:web": "expo start --web --host localhost",
    "eject": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit -p .",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn lint && yarn typecheck && yarn format:check && yarn jest"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-navigation/native": "^3.6.2",
    "@react-navigation/stack": "^5.0.0-alpha.37",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "exoflex": "^2.7.2",
    "expo": "~36.0.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@types/react-test-renderer": "^16.9.1",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.7.2",
    "eslint-config-kodefox": "^0.0.2",
    "jest": "^24.9.0",
    "jest-expo": "^36.0.1",
    "prettier": "^1.19.1",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.2.0",
    "typescript": "~3.6.3"
  },
  "eslintConfig": {
    "extends": "kodefox/react-native"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
